# 阿里云认证配置
# 请替换为您的实际阿里云AccessKey ID和AccessKey Secret
ACCESS_KEY_ID=your_access_key_id_here
ACCESS_KEY_SECRET=your_access_key_secret_here

# 默认地域配置，对具体集群等云资源所在的region会重新查询
# 阿里云容器服务支持的所有region列表为 https://help.aliyun.com/zh/ack/product-overview/supported-regions
REGION_ID=cn-hangzhou

# FastMCP日志级别
FASTMCP_LOG_LEVEL=INFO

# 缓存配置
CACHE_TTL=300
CACHE_MAX_SIZE=1000

# 开发环境配置
DEVELOPMENT=true